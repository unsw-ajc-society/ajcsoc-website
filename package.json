{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "ajcsoc-website",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"cf-typegen": "wrangler types --env example --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"dev": "next dev",
		"format": "oxfmt",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"start": "next start",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "1.16.5",
		"cloudflare-workers-and-google-oauth": "2.0.0",
		"daisyui": "5.5.19",
		"esbuild": "0.27.3",
		"next": "16.1.6",
		"next-intl": "4.8.3",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-icons": "5.5.0",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@risu729/tsconfigs": "2.0.0",
		"@tailwindcss/postcss": "4.2.0",
		"@types/node": "25.3.0",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"oxfmt": "0.34.0",
		"oxlint": "1.49.0",
		"tailwindcss": "4.2.0",
		"typescript": "5.9.3"
	}
}
